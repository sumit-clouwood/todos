{
  "name": "electron-todo-demo",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron.js",
  "productName": "electron-todo-demo",
  "homepage": "./",
  "build": {
    "extends": null,
    "productName": "electron-todo-demo",
    "appId": "com.electron-todo-demo.io",
    "copyright": "Â© 2021 electron-todo-demo",
    "files": [
      "./build/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 200,
          "type": "file"
        }
      ]
    },
    "afterSign": "notarize.js",
    "mac": {
      "icon": "public/logo512.png",
      "hardenedRuntime": true
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/logo512.png",
      "publisherName": "electron"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "icon": "public"
    }
  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "async": "^3.2.2",
    "aws-sdk": "^2.1030.0",
    "axios": "^0.24.0",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sqlite3": "^5.0.2",
    "uuidv4": "^6.2.12",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/async": "^3.2.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sqlite3": "^3.1.7",
    "autoprefixer": "9",
    "electron": "^16.0.0",
    "electron-builder": "^22.13.1",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.5",
    "postcss": "7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "postinstall": "install-app-deps",
    "electron:start": "electron public/electron.js",
    "buildwin": "npm run build && electron-builder build --win --x64",
    "buildmac": "npm run build && electron-builder -m",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}